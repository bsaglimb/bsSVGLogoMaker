{
  "name": "bssvglogomaker",
  "description": "## User Story",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "import": "esm",
  "directories": {
    "lib": "lib"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "esm": "^3.2.25",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "extensionsToTreatAsEsm": [".js"],
    "esModuleInterop": true
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "inquirer": "^9.2.12"
  },
  "compilerOptions": {
    "target": "node14",
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
